{
  "components": {
    "schemas": {
      "ConfigCall": {
        "type": "object",
        "description": "For getting a map of configurations for a specific configuration type.",
        "properties": {
          "filterModule": {
            "description": "Filter for modules in  {@link ConfigAction}",
            "type": "string"
          },
          "filterSubModule": {
            "description": "Filter for subModules in  {@link ConfigAction}",
            "type": "string"
          },
          "configType": {
            "description": "Key for configuration type.",
            "type": "string"
          },
          "configFilter": {
            "description": "Key for filter configuration",
            "type": "string"
          },
          "expose": {
            "description": "expose",
            "type": "string",
            "(ArrayList)enum": {
              "(Expose)0": "WEB",
              "(Expose)1": "INFO",
              "(Expose)2": "CREATE",
              "(Expose)3": "NONE"
            }
          },
          "sourcePath": {
            "description": "A sourcePath where EO offers it's input value when the execution starts.",
            "type": "string"
          },
          "targetPath": {
            "description": "A targetPath where the result of the execution will be mapped. Any combination of eo->placeholder. Could be set.",
            "type": "string"
          },
          "models": {
            "description": "A string representation for a list of model keys.",
            "type": "string"
          },
          "condition": {
            "description": "A condition for calls. ",
            "type": "string"
          },
          "startCondition": {
            "description": "A condition for calls. ",
            "type": "string"
          },
          "overwrite": {
            "description": "overwrite",
            "type": "boolean"
          },
          "logLevel": {
            "description": "logLevel",
            "type": "string",
            "(ArrayList)enum": {
              "(LogLevel)0": "DEBUG",
              "(LogLevel)1": "INFO_TEST",
              "(LogLevel)2": "INFO_QSU",
              "(LogLevel)3": "INFO_PROD",
              "(LogLevel)4": "INFO",
              "(LogLevel)5": "WARN",
              "(LogLevel)6": "ERROR"
            }
          },
          "duration": {
            "description": "The duration of a call.",
            "type": "integer",
            "format": "int62"
          },
          "prepend": {
            "description": "prepend String when executed by ExecutorCallImpl",
            "type": "string"
          },
          "postpend": {
            "description": "postpend String when executed by ExecutorCallImpl",
            "type": "string"
          },
          "id": {
            "description": "The id with a autonumbering",
            "type": "integer",
            "format": "int62"
          },
          "description": {
            "description": "A description of the model used by every model extending BaseClassImpl. ",
            "type": "string"
          },
          "naturalId": {
            "description": "The naturalKey for all caches {@link ConfigImpl} and {@link BeanImpl}",
            "type": "string"
          },
          "creationDate": {
            "description": "Used to define the creation of an item.",
            "type": "string",
            "format": "date-time"
          },
          "author": {
            "description": "The autor of something e,g. a class.",
            "type": "string"
          }
        }
      }
    }
  },
  "(LogLevel)_logLevel": "WARN"
}