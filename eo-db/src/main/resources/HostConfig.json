{
  "h2:file:basic": {
    "configModelKey": "DbConfig",
    "module": "eo-db",
    "moduleScope": "main",
    "description": "Local H2 in file persisted for the test resources.",
    "expose": "WEB",
    "properties": {
      "classPath": "src.main.resources",
      "schema": "file:basic",
      "dbType": "H2",
      "extension": "DATABASE_TO_UPPER=false;",
      "driver": "org.h2.Driver",
      "protocol": "jdbc",
      "hostName": "h2"
    },
    "rolePermissions": {
      "read": "guest"
    }
  },
